{
  "name": "plantich-web",
  "version": "1.0.0",
  "private": true,
  "description": "Plantich - The Sacred Alchemy of Plants. A Nordic luxury herbal apothecary platform.",
  "keywords": [
    "herbal medicine",
    "natural remedies",
    "medicinal plants",
    "holistic wellness",
    "next.js",
    "typescript",
    "tailwindcss"
  ],
  "author": "Plantich",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true npm run build",
    "export": "next build && next export",
    "test": "echo \"No tests specified\" && exit 0",
    "prepush": "npm run lint && npm run typecheck && npm run build",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@sanity/client": "^7.9.0",
    "@sanity/image-url": "^1.1.0",
    "googleapis": "^156.0.0",
    "next": "15.5.0",
    "next-sanity": "^10.0.13",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "husky": "^8.0.0"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "packageManager": "npm@10.x"
